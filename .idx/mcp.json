{
  "mcpServers": {
    "firebase": {
      "command": "npx",
      "args": [
        "-y",
        "firebase-tools@latest",
        "experimental:mcp",
        "--only",
        "auth,firestore"
      ]
    }
  }
}

{
  "mcpServers": {
    "genkit": {
      "command": "genkit",
      "args": ["mcp"],
      "cwd": "",
      "timeout": 30000,
      "trust": false
    }
  }
}